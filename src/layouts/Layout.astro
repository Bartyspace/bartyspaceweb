---
import ResponsiveNav from '../elements/ResponsiveNav.astro';
import HTML from '../elements/HTML.astro';
import LandingPage from '../sections/Main.astro';
import About from '../sections/About.astro';
import Projects from '../sections/Projects.astro';
import Services from '../sections/HireMe.astro';
import Footer from '../elements/Footer.astro';
---

<style is:global>
  @import url('https://fonts.cdnfonts.com/css/tt-lakes-neue-trial'); 
  @font-face {
    font-family: 'TT Lakes Neue Trial';
    font-display: swap;
    src: url(https://fonts.cdnfonts.com/css/tt-lakes-neue-trial);
  }
</style>
<HTML title="Inovativní disign a vývoj webových stránek">
  <ResponsiveNav />
  <LandingPage />
  <About />
  <Projects />
  <Services />
  <Footer />
  <script is:inline type="module" defer src="/JavaScripts/Grid.js"></script>
  <script is:inline type="text/partytown" defer src="/JavaScripts/Carousel.js"></script>
</HTML>
