---
import ResponsiveNav from '../elements/ResponsiveNav.astro';
import HTML from '../elements/HTML.astro';
import LandingPage from '../sections/Main.astro';
import About from '../sections/About.astro';
import Projects from '../sections/Projects.astro';
import Services from '../sections/HireMe.astro';
import Footer from '../elements/Footer.astro';
import { SEO, type Props as SEOProps } from 'astro-seo';
const { seo } = Astro.props;
interface Props {
  seo?: SEOProps;
}
---

<style is:global>
  @import url('https://fonts.cdnfonts.com/css/tt-lakes-neue-trial');
  @font-face {
    font-family: 'TT Lakes Neue Trial';
    font-display: swap;
    src: url(https://fonts.cdnfonts.com/css/tt-lakes-neue-trial);
  }
</style>
<HTML
  seo={{
    title: 'Bartyspace Labs',
    description:
      'Explore the universe of web innovation at BartySpaceLabs - your digital portal for cutting-edge web development and design solutions. Specializing in creating dynamic, user-centric experiences, our website pushes the boundaries of technology to bring your online vision to life with unparalleled creativity and technical expertise. Visit bartyspacelabs.netlify.app to embark on your journey to stellar online presence',
  }}
>
  <ResponsiveNav />
  <LandingPage />
  <About />
  <Projects />
  <Services />
  <Footer />
</HTML>
